<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    tools:parentTag="android.widget.LinearLayout">

    <com.kin.ecosystem.widget.TouchIndicatorIcon
        android:id="@+id/settings_icon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/kinecosystem_main_medium_margin"
        android:layout_marginRight="@dimen/kinecosystem_main_margin"/>

    <TextView
        android:id="@+id/settings_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1.0"
        style="@style/KinecosysSubTitle"
        android:textColor="@color/kinecosystem_black"/>

    <ImageView
        android:id="@+id/settings_arrow"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="@dimen/kinecosystem_main_medium_margin"
        android:src="@drawable/kinecosystem_arrow_right"
        />

</merge>